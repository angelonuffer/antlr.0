antlr = @ "../code/0"
unitest = @ "unitest.0"

test_input = "grammar Alts;
rule: 'a' | 'b' 'c' | 'd';"

result = antlr.parse({ input: test_input })

unitest.describe({
  name: "Alternatives Test"
  tests: [
    unitest.equals([ result.success 1 ])
    unitest.equals([ (result.success == 1 ? result.value.rules[.] : 0) 1 ])
  ]
})
