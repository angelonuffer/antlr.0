antlr = @ "../code/0"
unitest = @ "unitest.0"

test_input = "lexer grammar L;
fragment DIGIT: [0-9];
WS: [ ] -> skip;
ID: [a-z]+;"

result = antlr.parse({ input: test_input })

unitest.describe({
  name: "Lexer Features Test"
  tests: [
    unitest.equals([ result.success 1 ])
    unitest.equals([ (result.success == 1 ? result.value.rules[.] : 0) 3 ])
  ]
})
